<section>
    
        <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Lab Report Number</th>
      <th scope="col">Name</th>
      <th scope="col">Description</th>
      
    </tr>
  </thead>
  <tbody>
    {{#each labs}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.Name}}</td>
      <td>{{this.Category}}</td>
      <td>{{this.Price}}</td>
       <td><img  style="width:50px;height:50px;"src="/lab-images/{{this._id}}.jpg"alt=""></td>
      <td>
        
        <a href="/admin/delete-lab/{{this._id}}"class="btn btn-danger"onclick=" return confirm('Are you want to delete {{this.Name}} ?')">Delete</a>
        </td>
    </tr>
    {{/each}}