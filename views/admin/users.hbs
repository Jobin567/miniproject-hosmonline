<section>
    <div class="container">
        <div class="row mt-4">
            
        </div>
        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col">Date</th>
                    <th>Name</th>
                    <th>Address</th>
                    <th>Pincode</th>
                    
                    <th>Doctors Name</th>
                    
                    <th>Mobile</th>
                    <th>Weight</th>
                    <th>Age</th>
                    
                    
                </tr>
            </thead>
            <tbody>
                {{#each books}}
                <tr>
                    <td>{{this.date}}</td>
                    <td>{{this.bookDetails.name}}</td>
                    <td>{{this.bookDetails.address}}</td>
                    <td>{{this.bookDetails.pincode}}</td>
                    
                    <td>{{this.bookDetails.doctorname}}</td>
                    
                    <td>{{this.bookDetails.mobile}}</td>
                    <td>{{this.bookDetails.weight}}</td>
                    <td>{{this.bookDetails.age}}</td>
                   
                    
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>